{
  "compilerOptions": {
    "declaration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "jsx": "react",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "plugins": [
      {
        "name": "typescript-styled-plugin"
      }
    ]
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
